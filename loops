public class sadness
{
   static void sadness()
    {
        int a=1;
         while(a%7!=0)
     {
      System.out.println(a);
      a++;
    }
     for(int i=0;i<8; i++)
     {
         System.out.println(i);
        }
        int b = 0;
    do {
    System.out.println(b);
        b++;
    }
    while (b < 5);
    }
}
